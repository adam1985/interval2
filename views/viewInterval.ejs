<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta http-equiv="content-type" content="text/html;charset=utf-8">
    <link rel='stylesheet' href='/assets/styles/style.css' />
</head>
<body>
<div class="wrapper" id="add-plat">
    <%- include nav %>
    <div class="fl page-wrap">
            <ul class="view-interval-list">
                <% modeText = ["重复执行", "执行一次"]; %>
                <% for(var attr1 in data ) { %>
                <li>
                    用户：<%= attr1 %>
                    <ul>
                        <% for( var attr2 in data[attr1] ){ %>
                            <li>
                                公众平台：<%= attr2 %>
                                <ul>
                                    <% for( var attr3 in data[attr1][attr2] ){ %>
                                        <li>
                                            执行方式：<%= modeText[+attr3] %>
                                            <ul>
                                                <% for( var attr4 in data[attr1][attr2][attr3] ){ %>
                                                <li>
                                                    定时任务ID：<%= attr4 %>
                                                </li>
                                                <% } %>
                                            </ul>
                                        </li>
                                    <% } %>
                                </ul>
                            </li>
                        <% } %>
                    </ul>
                </li>
                <% } %>


            </ul>
    </div>
</div>
<script src="/assets/require.min.js" data-main="/assets/src/index"></script>
</body>
</html>